{
    "name": "mindfulindustries/php-http-transport",
    "description": "Simple PHP Guzzle wrapper.",
    "keywords": ["http", "guzzle", "php", "wrapper"],
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "guzzlehttp/guzzle": "^6.0"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "*",
        "laravel/lumen-framework": "5.7.*",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
